<table class="table table-stripped">
  <thead>
    <tr>
    <th>Название компании</th>
    <th>Город</th>
    <th>Email</th>
    <th>Телефон</th>
    <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%=user.name%></td>
      <td><%=user.lastname%></td>
      <td><%=user.email%></td>
      <td><%=user.phone%></td>
      <td><%= link_to 'Открыть', "show/#{user.id}" %></td>
    </tr>
  <% end %>
  </tbody>
</table>
